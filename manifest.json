{
  "manifest_version": 3,
  "name": "Help extension",
  "version": "0.0.1",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "permissions": ["scripting", "webNavigation"]
}
